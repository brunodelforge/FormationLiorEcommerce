{% extends 'base.html.twig' %}

{% block title %}Créer une catégorie
{% endblock %}

{% block body %}
	<h1>Créer une catégorie</h1>
	{{form_start(formView)}}
	<div class="row">
		{{form_row(formView.name)}}

	</div>
	<button type="submit" href="" class="btn btn-primary">
		<i class="fa fa-save">
			Enregistrer</i>
	</button>

	{{form_end(formView)}}
{% endblock %}
